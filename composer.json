{
    "name": "paloma/shop-bundle",
    "description": "Symfony bundle for Paloma web shop",
    "type": "library",
    "require": {
        "ext-json": "*",
        "ext-dom": "*",
        "ext-simplexml": "*",
        "paloma/shop-client": "^3.1.0",
        "symfony/serializer": "^4.3,<4.4",
        "symfony/property-access": "^4.3,<4.4",
        "symfony/security": "^4.3,<4.4",
        "symfony/security-bundle": "^4.3,<4.4",
        "twig/twig": "^2.9"
    },
    "require-dev": {
        "symfony/framework-bundle": "^4.3,<4.4",
        "symfony/test-pack": "^1.0",
        "symfony/phpunit-bridge": "^4.3,<4.4",
        "phpunit/phpunit": "^8.1"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Astina",
            "email": "admin@astina.ch"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Paloma\\ShopBundle\\": ""
        },
        "exclude-from-classmap": [
            "Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Paloma\\ShopBundle\\Tests\\": "Tests/"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../paloma-shop-client-php"
        }
    ]
}
